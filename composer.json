{
    "name": "chebur/controller-traits",
    "type": "library",
    "description": "Some symfony controller constraints",
    "keywords": [
        "symfony",
        "controller"
    ],
    "homepage": "https://github.com/vchebotarev",
    "license": "MIT",
    "authors": [
        {
            "name": "Vladimir Chebotarev",
            "homepage": "http://github.com/vchebotarev",
            "email": "v.yu.chebotarev@mail.ru"
        }
    ],
    "autoload": {
        "psr-4": {
            "Chebur\\ControllerTraits\\": "src"
        }
    },
    "require": {
        "php": "^7.1.3",
        "symfony/framework-bundle": "^3.4 || ^4.4 || ^5.0"
    },
    "suggest": {
        "doctrine/doctrine-bundle": "For using ControllerTraitDoctrine",
        "doctrine/orm": "For using ControllerTraitDoctrine",
        "jms/serializer-bundle": "For using ControllerTraitSerializerJms",
        "symfony/form": "For using ControllerTraitForm",
        "symfony/routing": "For using ControllerTraitRouter",
        "symfony/security-bundle": "For using ControllerTraitSecurity",
        "symfony/serializer": "For using ControllerTraitSerializer",
        "symfony/twig-bundle": "For using ControllerTraitTwig",
        "symfony/validator": "For using ControllerTraitValidator"
    }
}
